#. type: SS
#: condinapt-minios.7:139
#, no-wrap
msgid "Multimedia Module"
msgstr "Модуль мультимедиа"

#. type: Plain text
#: condinapt-minios.7:153
#, no-wrap
msgid ""
"\\f[C]# Additional codecs - not for minimal variant\n"
"gstreamer1.0-plugins-bad -pv=minimum\n"
"gstreamer1.0-plugins-ugly -pv=minimum\n"
"gstreamer1.0-libav -pv=minimum\\fR\n"
msgstr ""
"\\f[C]# Дополнительные кодеки - не для минимального варианта\n"
"gstreamer1.0-plugins-bad -pv=minimum\n"
"gstreamer1.0-plugins-ugly -pv=minimum\n"
"gstreamer1.0-libav -pv=minimum\\fR\n"

#. type: Plain text
#: condinapt-minios.7:161
#, no-wrap
msgid ""
"\\f[C]# Professional tools - only for toolbox and ultra\n"
"audacity +{pv=toolbox|pv=ultra}\n"
"kdenlive +{pv=toolbox|pv=ultra} +{de=xfce|de=lxqt}\n"
"---\n"
"# Distribution-specific packages from backports\n"
"ffmpeg \\[at]bookworm-backports +d=bookworm\\fR\n"
"\n"
msgstr ""
"\\f[C]# Профессиональные инструменты - только для toolbox и ultra\n"
"audacity +{pv=toolbox|pv=ultra}\n"
"kdenlive +{pv=toolbox|pv=ultra} +{de=xfce|de=lxqt}\n"
"---\n"
"# Пакеты для конкретного дистрибутива из backports\n"
"ffmpeg \\[at]bookworm-backports +d=bookworm\\fR\n"
"\n"

#. type: SS
#: condinapt-minios.7:162
#, no-wrap
msgid "Driver Module"
msgstr "Модуль драйверов"

#. type: Plain text
#: condinapt-minios.7:171
#, no-wrap
msgid ""
"\\f[C]\n"
"# Basic drivers\n"
"mesa-utils\n"
"xserver-xorg-video-all\\fR\n"
msgstr ""
"\\f[C]\n"
"# Основные драйверы\n"
"mesa-utils\n"
"xserver-xorg-video-all\\fR\n"

#. type: Plain text
#: condinapt-minios.7:174
#, no-wrap
msgid ""
"\\f[C]# NVIDIA drivers - only for non-free distributions\n"
"nvidia-driver +d=bookworm -{pv=minimum&de=core}\\fR\n"
msgstr ""
"\\f[C]# Драйверы NVIDIA - только для несвободных дистрибутивов\n"
"nvidia-driver +d=bookworm -{pv=minimum&de=core}\\fR\n"

#. type: Plain text
#: condinapt-minios.7:178
#, no-wrap
msgid ""
"\\f[C]# AMD drivers - for modern distributions\n"
"firmware-amd-graphics +{d=trixie|d=noble}\n"
"mesa-vulkan-drivers +{d=trixie|d=noble}\\fR\n"
msgstr ""
"\\f[C]# Драйверы AMD - для современных дистрибутивов\n"
"firmware-amd-graphics +{d=trixie|d=noble}\n"
"mesa-vulkan-drivers +{d=trixie|d=noble}\\fR\n"

#. type: Plain text
#: condinapt-minios.7:182
#, no-wrap
msgid ""
"\\f[C]# Old drivers - for old systems\n"
"xserver-xorg-video-radeon +d=bookworm\\fR\n"
"\n"
msgstr ""
"\\f[C]# Старые драйверы - для старых систем\n"
"xserver-xorg-video-radeon +d=bookworm\\fR\n"
"\n"

#. type: SS
#: condinapt-minios.7:183
#, no-wrap
msgid "Localization Module"
msgstr "Модуль локализации"

#. type: Plain text
#: condinapt-minios.7:191
#, no-wrap
msgid ""
"\\f[C]\n"
"# Basic locales - always\n"
"locales\\fR\n"
msgstr ""
"\\f[C]\n"
"# Основные локали - всегда\n"
"locales\\fR\n"

#. type: Plain text
#: condinapt-minios.7:196
#, no-wrap
msgid ""
"\\f[C]# Russian localization\n"
"language-pack-ru +lo=ru_RU\n"
"fonts-liberation +lo=ru_RU\n"
"firefox-esr-l10n-ru +lo=ru_RU +{de=xfce|de=lxqt}\\fR\n"
msgstr ""
"\\f[C]# Русская локализация\n"
"language-pack-ru +lo=ru_RU\n"
"fonts-liberation +lo=ru_RU\n"
"firefox-esr-l10n-ru +lo=ru_RU +{de=xfce|de=lxqt}\\fR\n"

#. type: Plain text
#: condinapt-minios.7:200
#, no-wrap
msgid ""
"\\f[C]# Multilingual support\n"
"task-russian +ml=true +lo=ru_RU\n"
"hunspell-ru +ml=true +lo=ru_RU\\fR\n"
msgstr ""
"\\f[C]# Многоязычная поддержка\n"
"task-russian +ml=true +lo=ru_RU\n"
"hunspell-ru +ml=true +lo=ru_RU\\fR\n"

#. type: Plain text
#: condinapt-minios.7:203
#, no-wrap
msgid ""
"\\f[C]# Keeping locales\n"
"vlc-l10n +kl=true +{pv=toolbox|pv=ultra}\\fR\n"
msgstr ""
"\\f[C]# Сохранение локалей\n"
"vlc-l10n +kl=true +{pv=toolbox|pv=ultra}\\fR\n"

#. type: Plain text
#: condinapt-minios.7:209
#, no-wrap
msgid ""
"\\f[C]# Regional settings for different locales\n"
"language-pack-pt +lo=pt_BR\n"
"language-pack-de +lo=de_DE\n"
"language-pack-fr +lo=fr_FR\\fR\n"
"\n"
msgstr ""
"\\f[C]# Региональные настройки для различных локалей\n"
"language-pack-pt +lo=pt_BR\n"
"language-pack-de +lo=de_DE\n"
"language-pack-fr +lo=fr_FR\\fR\n"
"\n"

#. type: SS
#: condinapt-minios.7:210
#, no-wrap
msgid "Advanced Filter Usage"
msgstr "Расширенное использование фильтров"

#. type: Plain text
#: condinapt-minios.7:219
#, no-wrap
msgid ""
"\\f[C]\n"
"# DKMS modules with kernel and distribution conditions\n"
"ntfs3-dkms -ka=true -d=buster -d=trixie -d=sid\n"
"zfs-dkms +{pv=toolbox|pv=ultra} +da=amd64 +kbd=true -kf=none\\fR\n"
msgstr ""
"\\f[C]\n"
"# Модули DKMS с условиями для ядра и дистрибутива\n"
"ntfs3-dkms -ka=true -d=buster -d=trixie -d=sid\n"
"zfs-dkms +{pv=toolbox|pv=ultra} +da=amd64 +kbd=true -kf=none\\fR\n"

#. type: Plain text
#: condinapt-minios.7:223
#, no-wrap
msgid ""
"\\f[C]# Drivers for old systems\n"
"broadcom-sta-dkms -d=jammy -ka=true -da=i386\n"
"aufs-dkms +dt=debian +d=buster\\fR\n"
msgstr ""
"\\f[C]# Драйверы для старых систем\n"
"broadcom-sta-dkms -d=jammy -ka=true -da=i386\n"
"aufs-dkms +dt=debian +d=buster\\fR\n"

#. type: Plain text
#: condinapt-minios.7:227
#, no-wrap
msgid ""
"\\f[C]# Exclusion for new distributions\n"
"realtek-rtl8821cu-dkms -d=trixie -d=sid\n"
"firmware-b43-installer -d=bionic\\fR\n"
msgstr ""
"\\f[C]# Исключение для новых дистрибутивов\n"
"realtek-rtl8821cu-dkms -d=trixie -d=sid\n"
"firmware-b43-installer -d=bionic\\fR\n"

#. type: Plain text
#: condinapt-minios.7:230
#, no-wrap
msgid ""
"\\f[C]# Complex alternatives with filters\n"
"exfatprogs -pv=minimum || exfat-utils -pv=minimum && exfat-fuse -pv=minimum\\fR\n"
msgstr ""
"\\f[C]# Сложные альтернативы с фильтрами\n"
"exfatprogs -pv=minimum || exfat-utils -pv=minimum && exfat-fuse -pv=minimum\\fR\n"

#. type: Plain text
#: condinapt-minios.7:237
#, no-wrap
msgid ""
"\\f[C]# Localization with exclusions and conditions\n"
"vlc-l10n -lo=en_US +{pv=toolbox|pv=ultra}\n"
"language-pack-gnome-ru-base +lo=ru_RU +dt=ubuntu\n"
"language-pack-gnome-ru-base +ml=true +dt=ubuntu\n"
"language-pack-gnome-ru-base +kl=true +dt=ubuntu\\fR\n"
"\n"
msgstr ""
"\\f[C]# Локализация с исключениями и условиями\n"
"vlc-l10n -lo=en_US +{pv=toolbox|pv=ultra}\n"
"language-pack-gnome-ru-base +lo=ru_RU +dt=ubuntu\n"
"language-pack-gnome-ru-base +ml=true +dt=ubuntu\n"
"language-pack-gnome-ru-base +kl=true +dt=ubuntu\\fR\n"
"\n"

#. type: SS
#: condinapt-minios.7:238
#, no-wrap
msgid "Optimization for MiniOS"
msgstr "Оптимизация для MiniOS"

#. type: Plain text
#: condinapt-minios.7:241
msgid "Grouping by functionality:"
msgstr "Группировка по функциональности:"

#. type: Plain text
#: condinapt-minios.7:247
#, no-wrap
msgid ""
"\\f[C]\n"
"#=== Core System ===\n"
"systemd +pv=standard +pv=toolbox +pv=ultra\n"
"dbus\\fR\n"
msgstr ""
"\\f[C]\n"
"#=== Базовая система ===\n"
"systemd +pv=standard +pv=toolbox +pv=ultra\n"
"dbus\\fR\n"

#. type: Plain text
#: condinapt-minios.7:252
#, no-wrap
msgid ""
"\\f[C]#=== Desktop Environment ===\n"
"xfce4-panel +de=xfce\n"
"lxqt-panel +de=lxqt\n"
"fluxbox +de=flux\\fR\n"
msgstr ""
"\\f[C]#=== Рабочее окружение ===\n"
"xfce4-panel +de=xfce\n"
"lxqt-panel +de=lxqt\n"
"fluxbox +de=flux\\fR\n"

#. type: Plain text
#: condinapt-minios.7:258
#, no-wrap
msgid ""
"\\f[C]#=== Applications by Variant ===\n"
"firefox-esr +{pv=standard|pv=toolbox|pv=ultra}\n"
"thunderbird +{pv=toolbox|pv=ultra}\n"
"libreoffice +pv=ultra\\fR\n"
"\n"
msgstr ""
"\\f[C]#=== Приложения по вариантам ===\n"
"firefox-esr +{pv=standard|pv=toolbox|pv=ultra}\n"
"thunderbird +{pv=toolbox|pv=ultra}\n"
"libreoffice +pv=ultra\\fR\n"
"\n"

#. type: SH
#: condinapt-minios.7:259
#, no-wrap
msgid "INTEGRATION WITH BUILD PROCESS"
msgstr "ИНТЕГРАЦИЯ С ПРОЦЕССОМ СБОРКИ"

#. type: SS
#: condinapt-minios.7:260
#, no-wrap
msgid "Module Environment Variables"
msgstr "Переменные окружения модуля"

#. type: Plain text
#: condinapt-minios.7:263
msgid ""
"Within the MiniOS module context, the following variables are available:"
msgstr "В контексте модуля MiniOS доступны следующие переменные:"

#. type: TP
#: condinapt-minios.7:263
#, no-wrap
msgid "\\f[B]$CWD\\fR"
msgstr "\\f[B]$CWD\\fR"

#. type: Plain text
#: condinapt-minios.7:266
msgid "Current module directory"
msgstr "Текущая директория модуля"

#. type: TP
#: condinapt-minios.7:266
#, no-wrap
msgid "\\f[B]$LIVEKITNAME\\fR"
msgstr "\\f[B]$LIVEKITNAME\\fR"

#. type: Plain text
#: condinapt-minios.7:269
msgid "System name (usually \\[lq]minios\\[rq])"
msgstr "Имя системы (обычно \\[lq]minios\\[rq])"

#. type: TP
#: condinapt-minios.7:269
#, no-wrap
msgid "\\f[B]$MODULE\\fR"
msgstr "\\f[B]$MODULE\\fR"

#. type: Plain text
#: condinapt-minios.7:272
msgid "Current module name"
msgstr "Имя текущего модуля"

#. type: SS
#: condinapt-minios.7:272
#, no-wrap
msgid "Using minioslib"
msgstr "Использование minioslib"

#. type: Plain text
#: condinapt-minios.7:275
msgid "Example installation script with conditional execution:"
msgstr "Пример скрипта установки с условным выполнением:"

#. type: Plain text
#: condinapt-minios.7:289
#, no-wrap
msgid ""
"\\f[C]# Check module conditions\n"
"if [ \\[dq]$PACKAGE_VARIANT\\[dq] = \\[dq]minimum\\[dq] ] && [ \\[dq]$DESKTOP_ENVIRONMENT\\[dq] = \\[dq]core\\[dq] ]; then\n"
"    echo \\[dq]Skipping module for minimal core build\\[dq]\n"
"    exit 0\n"
"fi\\fR\n"
msgstr ""
"\\f[C]# Проверка условий модуля\n"
"if [ \\[dq]$PACKAGE_VARIANT\\[dq] = \\[dq]minimum\\[dq] ] && [ \\[dq]$DESKTOP_ENVIRONMENT\\[dq] = \\[dq]core\\[dq] ]; then\n"
"    echo \\[dq]Пропуск модуля для минимальной сборки core\\[dq]\n"
"    exit 0\n"
"fi\\fR\n"

#. type: Plain text
#: condinapt-minios.7:296
#, no-wrap
msgid ""
"\\f[C]# Install packages\n"
"/usr/share/minios-live/condinapt \\[rs]\n"
"    -l \\[dq]$CWD/packages.list\\[dq] \\[rs]\n"
"    -c /etc/minios-live/build.conf \\[rs]\n"
"    -m /usr/share/minios-live/condinapt.map\\fR\n"
"\n"
msgstr ""
"\\f[C]# Установка пакетов\n"
"/usr/share/minios-live/condinapt \\[rs]\n"
"    -l \\[dq]$CWD/packages.list\\[dq] \\[rs]\n"
"    -c /etc/minios-live/build.conf \\[rs]\n"
"    -m /usr/share/minios-live/condinapt.map\\fR\n"
"\n"

#. type: TP
#: condinapt-minios.7:298
#, no-wrap
msgid "\\f[B]/etc/minios-live/build.conf\\fR"
msgstr "\\f[B]/etc/minios-live/build.conf\\fR"

#. type: Plain text
#: condinapt-minios.7:301
msgid "Main MiniOS build configuration file"
msgstr "Основной файл конфигурации сборки MiniOS"

#. type: TP
#: condinapt-minios.7:301
#, no-wrap
msgid "\\f[B]/usr/share/minios-live/condinapt.map\\fR"
msgstr "\\f[B]/usr/share/minios-live/condinapt.map\\fR"

#. type: Plain text
#: condinapt-minios.7:304
msgid "Filter mapping for MiniOS build system"
msgstr "Сопоставление фильтров для системы сборки MiniOS"

#. type: TP
#: condinapt-minios.7:304
#, no-wrap
msgid "\\f[B]/usr/share/minios-live/scripts/XX-module-name/packages.list\\fR"
msgstr "\\f[B]/usr/share/minios-live/scripts/XX-module-name/packages.list\\fR"

#. type: Plain text
#: condinapt-minios.7:307
msgid "Package list for each module"
msgstr "Список пакетов для каждого модуля"

#. type: TP
#: condinapt-minios.7:307
#, no-wrap
msgid "\\f[B]/usr/share/minios-live/scripts/XX-module-name/install\\fR"
msgstr "\\f[B]/usr/share/minios-live/scripts/XX-module-name/install\\fR"

