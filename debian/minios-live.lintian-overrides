# Boot files are architecture-dependent by design but placed in usr/share for live system
minios-live: arch-dependent-file-in-usr-share usr/share/minios-live/bootfiles/boot/grub/i386-efi/*
minios-live: arch-dependent-file-in-usr-share usr/share/minios-live/bootfiles/boot/grub/i386-pc/*
minios-live: arch-dependent-file-in-usr-share usr/share/minios-live/bootfiles/boot/grub/x86_64-efi/*
minios-live: arch-dependent-file-in-usr-share usr/share/minios-live/bootfiles/boot/syslinux/*

# ELF files in bootfiles are boot loaders, not regular executables
minios-live: unstripped-binary-or-object usr/share/minios-live/bootfiles/boot/syslinux/*
minios-live: unstripped-binary-or-object usr/share/minios-live/bootfiles/boot/grub/i386-efi/*
minios-live: unstripped-binary-or-object usr/share/minios-live/bootfiles/boot/grub/i386-pc/*
minios-live: unstripped-binary-or-object usr/share/minios-live/bootfiles/boot/grub/x86_64-efi/*

# Boot loader libraries have specific requirements
minios-live: library-not-linked-against-libc usr/share/minios-live/bootfiles/boot/syslinux/*
minios-live: missing-dependency-on-libc usr/share/minios-live/bootfiles/boot/grub/i386-pc/*
minios-live: shared-library-is-executable usr/share/minios-live/bootfiles/boot/syslinux/*
minios-live: statically-linked-binary usr/share/minios-live/bootfiles/boot/grub/i386-pc/*
minios-live: statically-linked-binary usr/share/minios-live/initramfs/livekit-mos/bin/*
minios-live: embedded-library usr/share/minios-live/bootfiles/boot/syslinux/*
minios-live: embedded-library usr/share/minios-live/initramfs/livekit-mos/bin/*
